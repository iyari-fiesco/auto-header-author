{
  "commentStyle": {
    ".js": { "start": "//", "end": "" },
    ".ts": { "start": "//", "end": "" },
    ".py": { "start": "#", "end": "" },
    ".go": { "start": "//", "end": "" },
    ".java": { "start": "//", "end": "" },
    ".css": { "type": "block", "start": "/*", "line": " *", "end": " */" },
    ".xml": {"type": "block", "start": "<#--", "end": "-->"}
  },
  "extensions": [".js", ".ts", ".py", ".go", ".java", ".css", ".xml"]
}
